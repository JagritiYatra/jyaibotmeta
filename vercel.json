{
  "version": 2,
  "builds": [
    {
      "src": "index.js",
      "use": "@vercel/node"
    },
    {
      "src": "web/public/**",
      "use": "@vercel/static"
    }
  ],
  "routes": [
    {
      "src": "/js/(.*)",
      "dest": "/web/public/js/$1"
    },
    {
      "src": "/data/(.*)",
      "dest": "/web/public/data/$1"
    },
    {
      "src": "/css/(.*)",
      "dest": "/web/public/css/$1"
    },
    {
      "src": "/profile-form.html",
      "dest": "/web/public/profile-form.html"
    },
    {
      "src": "/profile-form-enhanced.html",
      "dest": "/web/public/profile-form-enhanced.html"
    },
    {
      "src": "/plain-profile-form.html",
      "dest": "/web/public/plain-profile-form.html"
    },
    {
      "src": "/profile-setup",
      "dest": "/index.js",
      "headers": {
        "X-Frame-Options": "ALLOWALL",
        "Content-Security-Policy": "frame-ancestors 'self' https://*.whatsapp.com https://*.whatsapp.net https://*.facebook.com https://*.facebookwkhpilnemxj7asaniu7vnjjbiltxjqhye3mhbshg7kx5tfyd.onion",
        "X-Content-Type-Options": "nosniff"
      }
    },
    {
      "src": "/api/(.*)",
      "dest": "/index.js"
    },
    {
      "src": "/(.*)",
      "dest": "/index.js"
    }
  ],
  "env": {
    "NODE_ENV": "production"
  },
  "regions": ["bom1"]
}